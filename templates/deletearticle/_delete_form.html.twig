<form method="post" action="{{ path('article_delete', {'id': article.id}) }}" onsubmit="return confirm('Möchten Sie sicher dieses Blos    löschen?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ article.id) }}">
    <button class="btn btn-danger mt-2">Löschen</button>
</form>
